{
	"name": "laravel-boilerplate",
	"description": "A starting point for laravel application development",
	"homepage": "https://github.com/AndreasHeiberg/boilerplate",
	"version": "0.0.1",
	"main": "gruntfile.js",
	"author": {
		"name": "Andreas Heiberg",
		"url": "https://twitter.com/andheiberg",
		"role": "Developer"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:AndreasHeiberg/boilerplate.git"
	},
	"browser": {
		"underscore": "backbone/node_modules/underscore",
		"plugin": "./src/javascript/vendor/jquery-plugin.js"
	},
	"browserify": {
		"transform": [
			"browserify-shim"
		]
	},
	"browserify-shim": {
		"plugin": {
			"exports": "plugin",
			"depends": [
				"jquery:$"
			]
		}
	},
	"devDependencies": {
		"browserify-shim": "~3.4.1",
		"browserify": "~3.36.0",
		"vinyl-source-stream": "~0.1.1",
		"connect": "~2.14.3",
		"gulp-browserify": "~0.5.0",
		"gulp-changed": "~0.3.0",
		"gulp-imagemin": "~0.5.0",
		"gulp-livereload": "~1.5.0",
		"gulp-notify": "~1.2.5",
		"gulp-open": "~0.2.8",
		"gulp": "~3.6.0",
		"gulp-sass": "~0.7.1",
		"gulp-autoprefixer": "~0.0.7",
		"gulp-buster": "~0.2.1"
	},
	"dependencies": {
		"jquery": "~2.1.0"
	}
}